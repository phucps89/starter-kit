{
  "name": "ng-x-rocket",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^6.0.8",
    "@angular/common": "6.0.8",
    "@angular/compiler": "6.0.8",
    "@angular/core": "6.0.8",
    "@angular/forms": "6.0.8",
    "@angular/http": "6.0.8",
    "@angular/platform-browser": "6.0.8",
    "@angular/platform-browser-dynamic": "6.0.8",
    "@angular/router": "6.0.8",
    "@angular/service-worker": "6.0.8",
    "@ng-bootstrap/ng-bootstrap": "^2.2.2",
    "@ngx-translate/core": "^10.0.1",
    "admin-lte": "^2.4.8",
    "bootstrap": "3.3.5",
    "core-js": "^2.5.6",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "ng-http-loader": "^2.3.0",
    "ngx-toastr": "^9.0.2",
    "npm": "^6.4.1",
    "primeicons": "^1.0.0-beta.10",
    "primeng": "^6.1.3",
    "rxjs": "^6.2.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.6.8",
    "@angular/cli": "^6.0.8",
    "@angular/compiler-cli": "^6.0.8",
    "@angular/language-service": "^6.0.8",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@ngx-rocket/scripts": "^2.1.0",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "^2.0.2",
    "@types/lodash": "^4.14.116",
    "@types/node": "^8.10.28",
    "codelyzer": "^4.4.4",
    "hads": "^1.7.0",
    "htmlhint": "^0.9.13",
    "https-proxy-agent": "^2.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.2",
    "karma-junit-reporter": "^1.2.0",
    "protractor": "~5.3.0",
    "puppeteer": "^1.7.0",
    "stylelint": "~9.2.0",
    "stylelint-config-recommended-scss": "~3.2.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-scss": "^3.3.0",
    "ts-node": "~6.0.3",
    "tslint": "~5.10.0",
    "typescript": "~2.7.2"
  }
}
